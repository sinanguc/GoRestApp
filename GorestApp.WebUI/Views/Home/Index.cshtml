@model IEnumerable<GorestApp.Entities.Dto.Users.UserDto.WithId>
@{
    ViewData["Title"] = "Home Page";
}


<table id="dataTable" class="display">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Email</th>
            <th>Status</th>
            <th>Deleted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Email</td>
            <td>@item.Status</td>
            <td>@(item.Deleted ? "Evet" : "Hayır")</td>
        </tr>
        }
    </tbody>
</table>